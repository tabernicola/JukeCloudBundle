{
    "name": "tabernicola/jukecloud-bundle",
    "type": "symfony-bundle",
    "description": "Jukebox bundle for Symfony",
    "keywords": ["jukebox", "jukecloud", "music", "player", "playlist", "library"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tabernicola",
            "homepage": "https://github.com/tabernicola/JukeCloudBundle"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/tabernicola/jPlayer.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3",
        "components/bootstrap": "3.2.*",
        "components/font-awesome": "4.1.*",
        "components/jqueryui": "1.11.*",
        "components/jquery": "1.*",
        "components/jqueryui-theme-bootstrap": "*",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "blueimp/jquery-file-upload": "9.7.*",
        "stof/doctrine-extensions-bundle": "1.1.*",
        "dandelionmood/lastfm": "*",
        "liip/imagine-bundle": "1.0.*",
        "nass600/get-id3": "dev-master",
        "tabernicola/jPlayer": "2.7.*",
        "madcoda/php-youtube-api" : "dev-master",
        "components/jqueryui-theme-bootstrap": "dev-master"
    },
    "autoload": {
        "psr-0" : {
            "Tabernicola\\JukeCloudBundle" : ""
        }
    },
    "target-dir": "Tabernicola/JukeCloudBundle"
}

